{
  "routes": [
    {
      "route": "/admin/*",
      "rewrite": "/admin/index.html"
    },
    {
      "route": "/register/*",
      "rewrite": "/register/index.html"
    },
    {
      "route": "/bulk-import-template.csv",
      "headers": {
        "Content-Disposition": "attachment"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/assets/*.{js,css,png,gif}"]
  }
}
