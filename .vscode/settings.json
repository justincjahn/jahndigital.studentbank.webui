{
  "editor.tabSize": 2,
  "editor.useTabStops": false,
  "eslint.validate": [
    "javascript",
    "typescript",
    "vue"
  ],
  "cSpell.words": [
    "apexchart",
    "apexcharts",
    "Avenir",
    "certgen",
    "chonky",
    "composables",
    "customizability",
    "Denormalize",
    "fortawesome",
    "Jahn",
    "localstorage",
    "nameid",
    "Preauthorized",
    "Textbox",
    "utyp",
    "VITE",
    "xaxis",
    "yaxis"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.suggest.paths": false,
  "path-intellisense.mappings": {
      "@": "${workspaceFolder}/src",
  },
}
